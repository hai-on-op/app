        "@types/node": "latest",
        "@types/numeral": "latest",
        "@types/react": "latest",
        "@types/react-custom-scrollbars": "latest",
        "@types/react-dom": "latest",
        "@types/react-paginate": "latest",
        "@types/react-transition-group": "latest",
        "@types/styled-components": "latest",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "autoprefixer": "^10.4.15",
        "buffer": "^6.0.3",
        "dotenv": "^16.3.1",
        "eslint": "latest",
        "eslint-config-next": "^13.5.3",
