        "gh-pages": "latest",
        "i18next": "latest",
        "next": "latest",
        "numeral": "latest",
        "react": "latest",
        "react-confetti": "latest",
        "react-custom-scrollbars": "latest",
        "react-device-detect": "latest",
        "react-dom": "latest",
        "react-feather": "latest",
        "react-i18next": "latest",
        "react-jazzicon": "^1.0.4",
        "react-lottie-player": "latest",
        "react-paginate": "latest",
        "react-toastify": "latest",
        "react-tooltip": "latest",
        "react-transition-group": "latest",
        "styled-components": "latest",
        "viem": "1.6.7",
        "wagmi": "1.3.10"
